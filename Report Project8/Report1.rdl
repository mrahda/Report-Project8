  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>	select sfd_back_status_calc,sfd_sub_payment as DarounPayment, 0 as BorounPayment,1 as Daroun, RI.sfd_contract as DarounContract, null as BorounContract
	from sfd_real_installment RI inner join sfd_contract C on RI.sfd_contract = C.sfd_contractId
	
	where 
		RI.statecode = 0 
		and sfd_broker in ('{AF23817E-7B32-E911-80D2-000C29D9A3F6}','{2440D4EA-1276-E911-98E5-005056AE3285}')
		and sfd_status_of_cheqe in(3,31,32)
		and (sfd_emhdte_old_mali &lt; DATEADD(DAY, -1, GETDATE()) or sfd_emhdte_old_mali is null)
	
	Union all

	select sfd_back_status_calc,0 as DarounPayment, sfd_sub_payment as BorounPayment,0 as Daroun,null  as DarounContract, RI.sfd_contract as BorounContract
	from sfd_real_installment RI inner join sfd_contract C on RI.sfd_contract = C.sfd_contractId
	
	where 
		RI.statecode = 0 
		and sfd_broker not in ('{AF23817E-7B32-E911-80D2-000C29D9A3F6}','{2440D4EA-1276-E911-98E5-005056AE3285}')
		and sfd_status_of_cheqe in(3,31,32)
		and (sfd_emhdte_old_mali &lt; DATEADD(DAY, -1, GETDATE()) or sfd_emhdte_old_mali is null)

</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
