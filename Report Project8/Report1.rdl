        Function ToArabicNumber(txt As String) As String
        Dim i,j As Integer
        Dim c,result As String
        j = Len (txt)
        for i = 1 to j
        c = mid(txt,i,1)
        if Asc(c)  &gt;= 48 and Asc(c) &lt;=57 then
        result = result + ChrW (Val(c) +1776)
        else
        result = result + c
        end if
        next i
        Return result
        End Function


 Function FormatMoney(ByRef money As Decimal) As String
        Dim format As String
        Dim i, j As Integer
        Dim length As Integer
        Dim mony As String
        mony = money.ToString().Replace(".0000", "")
        format = ""
        j = 0
        length = Len(mony.ToString())
        If length &gt; 3 Then
            For i = length To 1 Step -1
                j = j + 1
                format = Mid(mony, i, 1) &amp; format
                If j Mod 3 = 0 And i &gt; 1 Then
                    format = "," &amp; format
                End If
            Next
            Return format
        Else
            Return money
        End If
    End Function

